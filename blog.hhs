{{!< default}}
    
    {{!content}}
    
    {{!-- Path: blog.hbs --}}
    {{!< default}}
    
    {{#each posts}}
      {{> post}}
    {{/each}}
    
    {{!-- Path: post.hbs --}}
    {{#with this}}
      <div class="post">
        <h2>{{title}}</h2>
        <p>{{description}}</p>
      </div>
    {{/with}}